<%- include ../../partials/header.ejs %> <%- include ../../partials/navbar.ejs
%> <%- include ../../partials/sidebar.ejs %>
<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <section class="content-header">
    <div class="container-fluid">
      <div class="row mb-2">
        <div class="col-sm-6">
          <h1>Kode Klasifikasi</h1>
        </div>
        <div class="col-sm-6">
          <ol class="breadcrumb float-sm-right">
            <li class="breadcrumb-item"><a href="/">Home</a></li>
            <li class="breadcrumb-item">
              <a href="/kodebuku">Kode Klasifikasi</a>
            </li>
            <li class="breadcrumb-item active">Edit</li>
          </ol>
        </div>
      </div>
    </div>
    <!-- /.container-fluid -->
  </section>

  <!-- Main content -->
  <section class="content">
    <div class="container-fluid">
      <div class="row">
        <div class="col-12">
          <div class="card card-primary">
            <div class="card-header">
              <h3 class="card-title">
                Form Edit Kode Klasifikasi "<%= kodebuku.kodedokumen %>"
              </h3>
            </div>
            <!-- /.card-header -->
            <!-- form start -->
            <form
              action="/kodebuku/edit/<%= kodebuku._id %>?_method=PUT"
              method="POST"
            >
              <div class="card-body">
                <div class="form-group">
                  <label for="kodedokumen">Kode Klasifikasi</label>
                  <input
                    type="text"
                    name="kodedokumen"
                    class="form-control"
                    id="kodebuku"
                    placeholder="Masukan kode klasifikasi"
                    value="<%= kodebuku.kodedokumen %>"
                  />
                </div>
              </div>
              <!-- /.card-body -->

              <div class="card-footer">
                <button type="submit" class="btn btn-primary">Simpan</button>
              </div>
            </form>
          </div>
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
  </section>
  <!-- /.content -->
</div>
<%- include ../../partials/footer.ejs %> <%- include ../../partials/js.ejs %>
